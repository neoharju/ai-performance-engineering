ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-13.0/bin/nvcc

cflags = -DTORCH_EXTENSION_NAME=warp_specialized_multistream_cuda -DTORCH_API_INCLUDE_EXTENSION_H -I/home/cfregly/ai-performance-engineering/code/ch11 -I/home/cfregly/ai-performance-engineering/code/common/headers -isystem /home/cfregly/.local/lib/python3.11/site-packages/torch/include -isystem /home/cfregly/.local/lib/python3.11/site-packages/torch/include/torch/csrc/api/include -isystem /usr/local/cuda-13.0/include -isystem /usr/include/python3.11 -fPIC -std=c++17
post_cflags = 
cuda_cflags = -DTORCH_EXTENSION_NAME=warp_specialized_multistream_cuda -DTORCH_API_INCLUDE_EXTENSION_H -I/home/cfregly/ai-performance-engineering/code/ch11 -I/home/cfregly/ai-performance-engineering/code/common/headers -isystem /home/cfregly/.local/lib/python3.11/site-packages/torch/include -isystem /home/cfregly/.local/lib/python3.11/site-packages/torch/include/torch/csrc/api/include -isystem /usr/local/cuda-13.0/include -isystem /usr/include/python3.11 -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr -gencode=arch=compute_121,code=sm_121 --compiler-options '-fPIC' -O3 -std=c++17
cuda_post_cflags = 
cuda_dlink_post_cflags = 
sycl_dlink_post_cflags = 
ldflags = -shared -L/home/cfregly/.local/lib/python3.11/site-packages/torch/lib -lc10 -lc10_cuda -ltorch_cpu -ltorch_cuda -ltorch -ltorch_python -L/usr/local/cuda-13.0/lib64 -lcudart

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc --generate-dependencies-with-compile --dependency-output $out.d $cuda_cflags -c $in -o $out $cuda_post_cflags





rule link
  command = $cxx $in $ldflags -o $out

build warp_specialized_multistream_extension.cuda.o: cuda_compile /home/cfregly/ai-performance-engineering/code/ch11/warp_specialized_multistream_extension.cu





build warp_specialized_multistream_cuda.so: link warp_specialized_multistream_extension.cuda.o

default warp_specialized_multistream_cuda.so
